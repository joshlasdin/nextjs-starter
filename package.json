{
    "name": "next-starter",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "greenkeeper": {
        "ignore": [
            "confidence"
        ]
    },
    "scripts": {
        "lint": "eslint src",
        "prettier": "prettier \"**/*.+(js|json|scss|css|yml|md)\"",
        "format": "yarn prettier --write",
        "validate": "yarn run lint && yarn run prettier --list-different",
        "test": "NODE_PATH=./src jest",
        "dev": "NODE_PATH=./src next ./src",
        "build": "NODE_PATH=./src next build ./src",
        "start": "NODE_PATH=./src next start ./src"
    },
    "browserslist": [
        ">1%",
        "last 4 versions",
        "Firefox ESR",
        "not ie < 9"
    ],
    "dependencies": {
        "@emotion/core": "^10.0.14",
        "@emotion/styled": "^10.0.14",
        "@testing-library/jest-dom": "^4.0.0",
        "@testing-library/react": "^8.0.4",
        "apollo-cache-inmemory": "^1.6.2",
        "apollo-client": "^2.6.3",
        "apollo-link-batch-http": "^1.2.12",
        "camelcase-keys": "^6.0.0",
        "confidence": "^3.0.0",
        "graphql": "^14.4.2",
        "graphql-tag": "^2.10.1",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "^4.17.14",
        "next": "^9.0.0",
        "postcss-import": "^12.0.1",
        "react": "^16.8.6",
        "react-apollo": "^2.5.8",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@zeit/next-css": "^1.0.1",
        "autoprefixer": "^9.6.1",
        "babel-eslint": "^10.0.2",
        "babel-plugin-emotion": "^10.0.14",
        "babel-plugin-graphql-tag": "^2.4.0",
        "dotenv-webpack": "^1.7.0",
        "eslint": "^6.0.1",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-emotion": "^10.0.14",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jest": "^22.7.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prefer-object-spread": "^1.2.1",
        "eslint-plugin-react": "^7.14.2",
        "format-messages-webpack-plugin": "^1.1.0",
        "husky": "^3.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.8.0",
        "lint-staged": "^9.2.0",
        "postcss-easy-import": "^3.0.0",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-nested": "^4.1.2",
        "prettier": "^1.18.2",
        "tailwindcss": "^1.0.5",
        "test-data-bot": "^0.8.0",
        "waait": "^1.0.5"
    }
}
