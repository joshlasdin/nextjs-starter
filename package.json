{
  "name": "next-starter",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "greenkeeper": {
    "ignore": [
      "confidence"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|json|scss|css|yml|md)\"",
    "format": "yarn prettier --write",
    "validate": "yarn run lint && yarn run prettier --list-different",
    "test": "NODE_PATH=./src jest",
    "dev": "NODE_PATH=./src next ./src",
    "build": "NODE_PATH=./src next build ./src",
    "export": "yarn build && next export -o build ./src",
    "start": "NODE_PATH=./src next start ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "camelcase-keys": "^6.2.2",
    "conf-store": "^1.3.2",
    "graphql": "^15.5.0",
    "lodash-es": "^4.17.21",
    "next": "^11.0.0",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^8.0.0",
    "postcss": "^8.3.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.1.4",
    "test-data-bot": "^0.8.0",
    "waait": "^1.0.5"
  }
}
